Old scoreboard stuff - could be useful for finding Marshals
org.bukkit.scoreboard.Scoreboard scoreboard = p.getScoreboard();

        Team bodies = null;

        for(Team team : scoreboard.getTeams()) {
            if(team.getName().equals("bodies")) {
                bodies = team;
                break;
            }
        }

        if(bodies == null) {
            bodies = scoreboard.registerNewTeam("bodies");
            bodies.setOption(Team.Option.NAME_TAG_VISIBILITY, Team.OptionStatus.NEVER);
        }

        bodies.addEntry(corpse.co());

